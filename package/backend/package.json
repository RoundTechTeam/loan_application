{
  "name": "nestjs-jwt-prisma-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prestart": "pnpm i && pnpm prisma:generate ",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "pnpm prestart && cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "gen:mobile": " ts-node -r tsconfig-paths/register ./libs/zod2dart/index.ts <path here>",
    "prisma:generate": "prisma generate && ts-node ./prisma/enum-generator.ts",
    "prisma:studio:dev": "dotenv -e .env.dev prisma studio",
    "prisma:studio:prod": "dotenv -e .env.prod prisma studio",
    "prisma:push:dev": "dotenv -e .env.dev prisma db push",
    "prisma:push:prod": "dotenv -e .env.prod prisma db push",
    "pub:docker:dev": "pnpm build && docker build . -f ./cicd/api.Dockerfile -t netsealtech/loan-dev-api:0.0.2 && docker push netsealtech/loan-dev-api:0.0.2",
    "pub:docker:prod": "pnpm build && docker build . -f ./cicd/api.Dockerfile -t netsealtech/loan-prod-api-prod:1.9.8 && docker push netsealtech/loan-prod-api-prod:1.9.8",
    "deploy:api:dev": "npx caprover deploy --appToken b04e9648af378bd7a4261a890bc4f09b3e1b0de122e21209292743dad3013692 --caproverUrl https://captain.dreamztrack.com.my --imageName docker.io/netsealtech/loan-dev-api:0.0.2 --appName loan-api-dev",
    "deploy:api:prod": "npx caprover deploy --appToken token_here --caproverUrl https://captain.dreamztrack.com.my --imageName docker.io/netsealtech/loan-api-prod:1.9.8 --appName loan-api-prod",
    "console:prod": "cross-env NODE_ENV=prod ts-node --files --project apps/cli/tsconfig.app.json -r tsconfig-paths/register apps/cli/src/main.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.332.0",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@prisma/client": "^5.7.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^4.14.0",
    "cache-manager": "^5.3.2",
    "cache-manager-ioredis-yet": "^1.2.2",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "groq-sdk": "^0.5.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "libphonenumber-js": "^1.10.36",
    "object-code": "^1.3.2",
    "openai": "^4.68.1",
    "mime-types": "^2.1.35",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-poppler": "^0.2.1",
    "prisma": "^5.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "short-unique-id": "^4.4.4",
    "twilio": "^4.23.0",
    "superjson": "^1.13.3",
    "tesseract.js": "^5.1.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@prisma/internals": "^5.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/multer": "^1.4.7",
    "@types/bcrypt": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@types/mime-types": "^2.1.1",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "nestjs-console": "^8.0.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "type-fest": "^4.4.0",
    "typescript": "^5.1.3",
    "zod-prisma-types": "^2.7.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~api/(.*)": "<rootDir>/src/$1",
      "~libs/(.*)": "<rootDir>/libs/$1"
    },
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ]
  }
}